#include <stdio.h>
#include <algorithm>
using namespace std;

bool cmp(pair<int,int> left,pair<int,int> right)
{
	if(left.first == right.first) return left.first > right.first
	return left.first < right.first;
}

int main()
{
	int n;
	scanf("%d",&n);
	
	pair <int,int> d[1001];
	for(int i = 0;i<n;i++)
	{
		int num;
		scanf("%d %d %d",&num,&d[i].first,&d[i].second);
	}
	
	sort(d,d+n,cmp);
	
	int 
	
}
