#include <stdio.h>
#include <algorithm>
using namespace std;

int main()
{
	int n,d,k,c;
	scanf("%d %d %d %d ",&n,&d,&k,&c);
	
	int a[101];
	for(int i = 0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	
	int max_list[n+1] = {0,};
	for(int i = n;i<n+k;i++)
	{
		a[i] = a[i%n];
	}
	
	for(int i = 0;i<n;i++)
	{
		int cnt = 0;
		int c = 0;
		for(int j = 0;j<k;j++)
		{
			if(a[i] == a[j]) cnt++;
		}
		if(cnt == 0) max_list[i] = i;
	} 
	
	sort(max_list,max_list + n);
	for(int i = 0;i<n;i++) printf("%d",max_list[i]);
	
}
