#include <stdio.h>
int main()
{
	int a[4];
	int b[4];
	int c = 0;
	int max = 0;
	
	for(int i = 0;i < 4;i++)
	{
		
		scanf("%d %d",&a[i],&b[i]);
		c = c + b[i] - a[i];
		if(c > max)
		{
			max = c;
		}
	}
	
	printf("%d",max);	
	
}
