#include <stdio.h>

int main()
{
	int a,b;
	scanf("%d %d",&a,&b);
	
	int p ,q;
	scanf("%d %d",&p,&q);
	int x = p,y = q;
	
	int t;
	scanf("%d",&t);
	
	int cycle = 0;
	
	for(int i = 0;i<t;i++)
	{
		int direction[2];
		direction[0] = 1;
		direction[1] = 1;
		p += direction[0];
		q += direction[1];
		if(p >= a || p <= 0) direction[0] * (-1);
		if(q >= a || q <= 0) direction[1] * (-1);
		if(p == x && q == y){
			cycle = i;
			t %= cycle;
		}
		t++;
	}
	
	printf("%d %d",p,q);
}
