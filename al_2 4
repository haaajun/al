#include <stdio.h>
#include <algorithm>
using namespace std; 

struct ball{
	int num;
	int col;
	int size;
	int sum = 0;
	int s_c = 0;
};

bool cmp(ball a,ball b)
{
	return a.size > b.size;
}

int main()
{
	int n;
	scanf("%d",&n);
	
	ball s[n+1];

	for(int i = 0;i < n;i++)
	{
		scanf("%d %d",&s[i].col,&s[i].size);
		s[i].num = i;
	}
	
	sort(s,s+n,cmp);
	
	for(int i = 0;i < n;i++)
	{
		for(int j = 0;j < i;j++)
		{
			if(s[i].col == s[j].col)
			{
				s[i].s_c += s[j].size;
			}
		}
	}
	
	for(int i = 0;i < n;i++)
	{
		for(int j = 0;j < i;j++)
		{
			s[i].sum += s[j].size;
		}
	}
	
	for(int i = 0;i < n;i++)
	{
		printf("%d \n",s[i].sum - s[i].s_c);
	}
	
	
}
