#include <cstdio>

int main()
{
	int a,b;
	scanf("%d %d",&a,&b);
	int c;
	int x[101] = {0,};
	
	
	scanf("%d",&c);
	for(int i = 0;i<c;i++)
	{
		scanf("%d",&x[i]);
	}
	
	int original = 0;
	int i = 0;
	while(x[i]){
		i++;
		original += x[i] * a^i;
	}
	

	printf("%d",original);
	
	
}
