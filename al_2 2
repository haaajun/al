#include <stdio.h>
#include <stack>
using namespace std;

int main()
{
	int n;
	int a[500001];
	stack <pair<int,int>> s;
	
	scanf("%d",&n);
	
	for(int i = 0;i < n;i++)
	{
		scanf("%d",&a[i]);
		s.push(a[i]);
	}
	

}
